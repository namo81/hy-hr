	0% { transform:rotate(0deg); }
	100% { transform:rotate(360deg); }
}

/* 스크롤바  =============================================================== */
/* webkit */
::-webkit-scrollbar { border:1px solid #d7d7d7; background:#f4f4f4; }
::-webkit-scrollbar-button:start:decrement {display: block; height: 15px; border:1px solid #d7d7d7; background:url(/images/img_scroll-up.png) no-repeat center 50%; }
::-webkit-scrollbar-button:end:increment {display: block; height: 15px; border:1px solid #d7d7d7; background:url(/images/img_scroll-down.png) no-repeat center 50%; }
::-webkit-scrollbar-track {background: #f4f4f4; -webkit-border-radius: 0px; border-radius:0px; border:1px solid #d7d7d7; }
::-webkit-scrollbar-thumb {height: 50px; width: 50px; background: rgba(0,0,0,.2); -webkit-border-radius: 0px; border-radius: 0px; border:1px solid #d7d7d7; background:#fff; }

/* IE */
html {scrollbar-3dLight-Color: #f4f4f4; scrollbar-arrow-color: #909090; scrollbar-base-color:#f4f4f4; scrollbar-Face-Color: #fff; scrollbar-Track-Color: #f4f4f4; scrollbar-DarkShadow-Color: #d7d7d7; scrollbar-Highlight-Color: #fff; scrollbar-Shadow-Color: #d7d7d7; }

