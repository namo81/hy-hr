@charset 'utf-8';

/* -- date : 2017-12-07 -- */
/* -- snamo(Seo nam ho) for m.s.p -- */
@import url(http://fonts.googleapis.com/earlyaccess/nanumgothic.css);
/* 2019-07-25 나눔고딕 추가 */
@import url(fonts.css); 

* { margin:0; padding:0; }
html * { font-family:'Nanum Gothic','맑은 고딕',Dotum,AppleGothic,sans-serif; }
html, body { font-size:12px; line-height:1.4em; }
ul,ol { display:inline-block; }
li, dd { list-style:none; }
a { color:inherit; text-decoration:none; }
a:hover, a:active, a:focus { text-decoration:none; }
em, var, dfn, cite, address,i  { font-style:normal; }
img { border:0px; }
fieldset { border:none; }
table { border-collapse:collapse; table-layout:fixed; width:100%; }
input, select, textarea { font-family:Roboto,Droid Sans,AppleSDGothicNeo,Helvetica,sans-serif; }
button { display:inline-block;overflow:hidden;border:0;background:none;cursor:pointer; }
button > span { position:relative; }
.hide { position:absolute; overflow:hidden; top:0; left:0; width:100%; height:1px; text-indent:-10000px; }
caption { height: 1px; overflow: hidden; line-height: 0; margin-top: -1px; color: transparent; }
textarea { resize:none; vertical-align:middle; }
*[tabindex] { outline:none; }
pre { white-space:pre-wrap; font-family:'Nanum Gothic','맑은 고딕',Dotum,AppleGothic,sans-serif; word-break:break-word; }

/* skip navi */
#skip-navi { position:relative; z-index:1000; }
#skip-navi > a { position:absolute; width:100%; height:30px; top:-50px; left:0; text-align:center; font-size:13px; line-height:30px; color:#fff; background:#444;}
#skip-navi > a:focus { top:0; }

/* layout =============================================================== */
#wrap { width:100%; min-width:1300px; height:100%; }

/* height 100% 설정 - layout-mix / layout-ver 용 */
html, body { height:100%; } 
#top { position:relative; height:58px; background:#df4837; z-index:100; }
#mid { position:relative; min-height:100%; margin:-52px 0 -60px; background:#eaedf2; }
#mid > .contents { height:100%; min-height:1100px; padding:52px 0 60px; }
#btm { position:relative; height:60px; background:#cfd3da; z-index:100; }

/* header 설정 -------------------- */
	/* top-menu */
.top-menu { height:58px; }
.top-menu .inbox { width:1200px; margin:0 auto; overflow:hidden; }
.top-menu .logo { float:left; width:215px; padding:18px 0 0 32px; text-align:left; }

.top-notice { position:relative; display:block; margin-top:14px; height:42px; overflow:hidden; text-align:right; }
.top-notice:after { content:''; position:absolute; display:block; width:18px; height:18px; top:12px; right:485px; background:url(/images/ico_notice.png) no-repeat left top; }
.top-notice a { display:block; color:#fff; overflow:hidden; }
.top-notice a > span { float:left; width:480px; height:42px; padding-left:16px; font-size:16px; line-height:42px; text-align:left; /* background:url(/images/ico_notice.png) no-repeat left 50%;  */overflow:hidden; text-overflow:ellipsis; white-space:nowrap; }
.top-notice a .ico-new { display:inline-block; height:42px; margin-left:5px; line-height:44px; vertical-align:middle; }

/* mid 설정 -------------------- */
.contents { width:1200px; margin:0 auto; }
.contents:after { content:''; display:block; clear:both; }

.side-wrap { position:absolute; width:215px; height:100%; top:0; padding-top:52px; background:#fff; border-left:1px solid #adadad; border-right:1px solid #adadad; box-sizing:border-box; }

.side-wrap .user-info { padding:34px 10px 35px; border-bottom:1px solid #acacac; text-align:center; }
.side-wrap .user-info .user-img { display:block; width:80px; height:80px; margin:0 auto 12px; border-radius:40px; overflow:hidden; }
.side-wrap .user-info .user-img > img { width:100%; }
.side-wrap .user-info .user-name { font-size:13px; line-height:1.4em; color:#393939; }
.side-wrap .user-info .user-name strong { display:block; font-size:15px; margin-bottom:6px; }

.side-wrap ul, .side-wrap li { width:100%; }
.side-wrap .depth1 { display:block; width:174px; margin:20px auto; }
.side-wrap .depth1 > li { margin-bottom:5px; }
.side-wrap .depth1 > li > a { position:relative; display:block; padding:12px 20px 12px 6px; font-size:15px; line-height:1.2em; color:#393939; border-bottom:1px solid #727272; font-weight:bold; }
.side-wrap .depth1 > li.gray a { color:#efe9e9; }
.side-wrap .depth1 > li > a:after { content:''; position:absolute; width:9px; height:6px; top:17px; right:9px; background:url(/images/ico_arrow-lnb.png) no-repeat left top; transition:.3s; }
.side-wrap .depth1 > li > a.alone:after { display:none; }
.side-wrap .depth1 > li.on > a, .side-wrap .depth1 > li > a:hover { font-weight:bold; color:#e14836; }
.side-wrap .depth1 > li.on > a:after { background-position:left bottom; }
.side-wrap .depth2 { display:block; padding:2px 0; }
.side-wrap .depth2 li { margin:8px 0; box-sizing:border-box; }
.side-wrap .depth2 a { display:block; padding:6px 20px 6px 29px; font-size:14px; line-height:1.2em; color:#646464; }
.side-wrap .depth2 a > i { display:inline-block; margin-right:4px; }
.side-wrap .depth2 li.on a, .side-wrap .depth2 li a:hover { font-weight:bold; background:#eaedf2; color:#1b1b1b; }
.side-wrap .depth2 li.divide { position:relative; margin-top:19px; }
.side-wrap .depth2 li.divide:after { content:''; position:absolute; width:148px; height:1px; top:-9px; left:13px; background:url(/images/ico_dotline-lnb.png) repeat-x left top; }
.side-wrap .depth2 li .ico-pos { display:inline-block; margin-left:3px; padding:0px 7px; font-size:11px; line-height:14px; color:#8d8d8d; background:#f5f5f5; border-radius:1em; border:1px solid #acacac; }

.side-wrap .side-etc-btns { display:block; width:174px; margin:17px auto; }
.side-etc-btns li { margin-bottom:9px; }
.side-etc-btns a { display:block; padding:6px 23px 7px 51px; font-size:12px; line-height:1.2em; font-weight:bold; color:#353535; border:1px solid #d9d8d9; border-radius:30px; box-sizing:border-box; background-repeat:no-repeat; background-position:19px 50%; }
.side-etc-btns a:hover { background-color:#fafbfc; }
.side-etc-btns a.btn-ehr { background-image:url(/images/ico_outlink.png); }
.side-etc-btns a.btn-system { background-image:url(/images/ico_setting.png); }
.side-etc-btns a.btn-admin { background-image:url(/images/ico_admin.png); }
.side-etc-btns a.btn-peer { background-image:url(/images/ico_peer-btn.png); letter-spacing:-0.05em; border-color:#338188; color:#338188; background-color:#f1fcfa; }
.side-etc-btns a.btn-resume { padding-left:20px; background:url(/images/ico_resume2.png) no-repeat 90% 50%; }
.side-etc-btns a.btn-my-org { padding-left:23px; border-color:#5b99de; color:#3d5b99; background-color:#e9faff; }
.side-etc-btns a.btn-infra { padding-left:23px; text-align:center; color:#784fa6; background-color:#f6eefd; border-color:#ac77e8;}

.cnt-wrap { display:block; margin-left:215px; padding:25px 0px 10px 20px; box-sizing:border-box; }
.cnt-area { display:block; margin-bottom:20px; padding:30px 25px; background:#fff; border:1px solid #acacac; }


/* btm 설정 -------------------- */
footer { width:1200px; margin:0 auto; padding:10px; font-size:10px; box-sizing:border-box; font-family:tahoma; text-align:center; color:#666; font-weight:bold; }
footer .footer-logo { display:inline-block; margin-right:10px; }

/* common ===============================================================*/
.tc { text-align:center !important; }
.tl { text-align:left !important; }
.tr { text-align:right !important; }

.fl { float:left; }
.fr { float:right; }

.tx-red { color:#ea002c !important; }
.tx-red2 { color:#e86c5f; }
.tx-blue { color:#0f82ed; }
.tx-yellow { color:#ffce22; }

.location { margin-top:20px; text-align:right; font-size:12px; line-height:1.4em; color:#696868; vertical-align:bottom; font-weight:normal; letter-spacing:0; }
.location span { display:inline-block; padding-right:10px; margin-right:2px; background:url(/images/ico_arrow-location.png) no-repeat right 1px top 75%; }

.shadow { box-shadow:1px 2px 0 rgba(0,0,0,.15); }

.ico-plus { position:relative; display:block; width:4px; height:20px; margin:0 auto; background:#bdbdbd; }
.ico-plus:after { content:''; position:absolute; width:20px; height:4px; top:8px; left:-8px; background-color:inherit; }

.flag { display:inline-block; margin-right:5px; padding-left:16px; font-weight:bold; color:#f65499; background:url(/images/ico_need-check.png) no-repeat left 50%; }
.flag.blue { background-image:url(/images/ico_flag-blue.png); color:#6c8ae1;}

/* 작게 들어가는 유저 이미지 */
.img { display:inline-block; width:28px; height:28px; border-radius:15px; overflow:hidden; vertical-align:middle; }
.img > img { width:100%; }

/* 나의 / 공통 task 상태표기 */
.kind-task { display:inline-block; font-size:12px; line-height:18px; font-weight:normal; color:#636363; padding-left:30px; background:url(/images/ico_solo.png) no-repeat left 50%; vertical-align:middle; }
.kind-task.with { background-image:url(/images/ico_together.png); }

/* 리스트 내 코멘트 아이콘 */
.comment { display:inline-block; padding-left:25px; font-size:13px; line-height:18px; color:#393939; font-weight:bold; background:url(/images/ico_comment.png) no-repeat left 50%; vertical-align:middle; }
.comment.new { background-image:url(/images/ico_comment-new.png); }

/* tips */
.tips .title { margin-bottom:15px; padding-left:20px; font-size:13px; color:#2b2a2a; line-height:1.4em; background:url(/images/ico_import.png) no-repeat left 50%; }
.tips li, .tips p { margin-bottom:6px; padding-left:10px; font-size:13px; line-height:1.4em; color:#535353; background:url(/images/ico_blet1.png) no-repeat left 7px; background-size:3px 3px; }
.tips .ico-tx { padding-left:20px; background:url(/images/ico_import.png) no-repeat left 50%;}

.box-like-tbl { display:table; width:100%; border:2px solid #b5b5b5; border-radius:5px; box-sizing:border-box; background:#fff; overflow:hidden; }
.box-like-tbl .title { display:table-cell; width:135px; vertical-align:middle; font-size:15px; line-height:1.4em; color:#353535; text-align:center; font-weight:bold; background:#fafbfc; border-right:1px solid #b4b4b4; border-radius:5px 0 0 5px; }
.box-like-tbl .title .desc { font-size:13px; }
.box-like-tbl .cnt { display:table-cell; padding:20px; }
.box-like-tbl + .box-like-tbl { margin-top:15px; }

.mate-list { margin:-15px 0 -19px; }
.mate-list li { float:left; width:40px; text-align:center; font-size:12px; line-height:1.4em; color:#575757; }
.mate-list li.dot { width:20px; font-weight:bold; line-height:40px; text-align:right; }
.mate-list li .img { display:block; margin:0 auto; }

.btn-top { position:fixed; width:34px; height:54px; left:50%; bottom:10%; margin-left:610px; padding-top:23px; background:#313131; color:#fff; font-size:14px; line-height:16px; text-align:center; border-radius:20px; box-sizing:border-box; box-shadow:1px 3px 6px rgba(0,0,0,.4);}
.btn-top:after { content:''; position:absolute; width:8px; height:1px; right:16px; top:17px; background:#5ae792; transform:rotate(-45deg);}
.btn-top:before { content:''; position:absolute; width:8px; height:1px; left:16px; top:17px; background:#5ae792; transform:rotate(45deg);}
.btn-top:hover { background-color:#ffa800; color:#000; }
.btn-top:hover:after, .btn-top:hover:before { background:#fff; }

/* 타이틀 및 기본 text / desc 설정 ===============================================================*/
.title-dep1 { display:block; height:45px; margin-bottom:18px; font-size:26px; line-height:45px; border-bottom:1px solid #cdcdcd; color:#393939; letter-spacing:-0.05em; }
.title-dep1:after { content:''; display:block; clear:both; }
.title-dep1 > .desc { display:inline-block; margin-left:5px; font-size:14px; line-height:1.4em; color:#666; }
.title-dep2 { display:block; margin:0 0 10px; font-size:18px; line-height:1.4em; letter-spacing:-0.05em; color:#393939; }
.title-dep2:after { content:''; display:block; clear:both; }
.title-dep2 > .desc { display:inline-block; margin-left:5px; font-size:13px; line-height:1.4em; vertical-align:bottom; color:#666; font-weight:normal;  letter-spacing:-0em; }
.title-dep3 { display:block; margin:0 0 10px; font-size:14px; line-height:1.4em; letter-spacing:-0.05em; }
.title-dep3:after { content:''; display:block; clear:both; }
.title-dep3 > .desc { display:inline-block; margin-left:5px; font-size:12px; line-height:1.4em; vertical-align:bottom; color:#666; }

.title-dep1 .fr, .title-dep2 .fr { float:right; }

.ico-tit { display:inline-block; height:28px; margin-right:5px; vertical-align:middle; }

.title-dep1.no-line { border-bottom:none; }

/* input =============================================================== */
input[type=text], input[type=password], input[type=date], input[type=number], textarea { display:inline-block; width:100%; height:33px; padding:8px 10px; border:1px solid #b5b5b5; font-size:13px;  line-height:17px; box-sizing:border-box; color:#535353; border-radius:4px; }
textarea { line-height:20px; }

select { height:33px; line-height:24px; border-radius:3px; }

input[type=text][readonly], input[type=number][readonly], textarea[readonly]  { background:#f2f3f5; }
input[type=text][readonly].normal, input[type=number][readonly].normal, textarea[readonly].normal  { background:#fff; }

.inp-set { display:block; }
.inp-set + .inp-set { margin-top:8px; }

.inp-set.phone:after { content:''; display:block; clear:both; }
.inp-set.phone > span, .inp-set.phone > .inp-select, .inp-set.phone > span, .inp-set.phone > .new-select { display:block; float:left; width:30%; }
.inp-set.phone > i { display:block; float:left; width:5%; text-align:center; line-height:34px; }

.inp-set.mail:after { content:''; display:block; clear:both; }
.inp-set.mail > span, .inp-set.mail > .inp-select, .inp-set.mail > span, .inp-set.mail > .new-select { display:block; float:left; width:30%; }
.inp-set.mail > i { display:block; float:left; width:5%; text-align:center; line-height:34px; }
.inp-set.mail .inp-select, .inp-set.mail .new-select { width:32%; margin-left:3%; }

.inp-set.address { overflow:hidden; }
.inp-set.address > input { margin-top:8px; }
.inp-set.address .post-num { overflow:hidden; }
.inp-set.address .post-num > span { float:left; width:120px; margin-right:5px; }

.inp-alert { display:block; clear:both; padding-top:5px; color:#ea002c; }

.max-tx { font-size:12px; color:#b5b5b5; }
input[type=text] + .max-tx { margin-left:5px; }
textarea + .max-tx { display:block; margin-top:3px; text-align:right; }

/* checkbox / radio 디자인 적용 - 미사용시 주석 처리 */
.inp-check, .inp-radio { position:relative; display:inline-block; overflow:hidden; min-height:26px; vertical-align:middle; }
.inp-check input, .inp-radio input { position:absolute; width:1px; height:1px; padding:0; top:3px; left:3px; z-index:-1; overflow:hidden; }
.inp-check label, .inp-radio label { float:left; cursor:pointer; padding-left:30px; outline:none; color:#2b2b2b; font-size:13px; line-height:25px; }
.inp-check label .box { position:absolute; width:25px; height:25px; top:0px; left:0px; background:url(/images/bg_checkbox.png) no-repeat left bottom; border:none;}
.inp-radio label .box { position:absolute; width:24px; height:24px; top:0px; left:0px; background:url(/images/bg_radio-new.png) no-repeat left bottom; border:none;}
.inp-check label.on .box, .inp-radio label.on .box { background-position:left top; }
.inp-check input:focus + label .box, .inp-radio input:focus + label .box { outline:1px dotted #999; outline:-webkit-focus-ring-color auto 5px; }
.inp-check + .inp-check, .inp-check + .inp-radio, .inp-radio + .inp-check, .inp-radio + .inp-radio { margin-left:15px; }

.inp-check.type2 { min-height:19px; vertical-align:-2px; }
.inp-check.type2 label { padding-left:23px; line-height:19px;}
.inp-check.type2.non-tx label { width:19px; line-height:19px; }
.inp-check.type2 label .box { width:18px; height:18px; background-image:url(/images/bg_checkbox2.png); }

.inp-radio.type2 { min-height:18px; }
.inp-radio.type2 label { padding-left:23px; line-height:18px;}
.inp-radio.type2.non-tx label { width:16px; line-height:18px; }
.inp-radio.type2 label .box { width:16px; height:16px; margin-left:0; background-image:url(/images/bg_radio.png); }

.inp-radio.type3 { min-height:22px; }
.inp-radio.type3 label { padding-left:23px; line-height:22px;}
.inp-radio.type3.non-tx label { width:20px; line-height:22px; }
.inp-radio.type3 label .box { width:20px; height:20px; background-image:url(/images/bg_radio-mid.png); }

.inp-check.pop { min-height:24px; }
.inp-check.pop label { padding-left:30px; font-size:14px; line-height:22px; color:#fff;}
.inp-check.pop label .box { width:24px; height:24px; background-image:url(/images/bg_checkbox-pop.png); }

.inp-check label.disabled .box, .inp-radio label.disabled .box, .inp-check.type2 label.disabled .box { opacity:.3; filter: alpha(opacity=30)}

.inp-check.non-tx label, .inp-radio.non-tx label { width:25px; padding-left:0; text-indent:-10000px; }

/* 토글형 checkbox */
.check-tgl { position:relative; width:68px; height:30px; }
.check-tgl input { position:absolute; width:1px; height:1px; padding:0; top:3px; left:3px; z-index:-1; overflow:hidden; }
.check-tgl label { position:relative; display:block; width:100%; height:100%; background:#e5e7ea; z-index:1; border-radius:15px; cursor:pointer; box-shadow:0 -1px 0px rgba(255,255,255,.9) inset; transition:.2s ease-in-out; }
.check-tgl label .round { position:absolute; width:25px; height:25px; top:2px; left:3px; background:#fff; border-radius:15px; box-shadow:0 2px 6px rgba(0,0,0,.5); transition:.2s ease-in-out; transform:translate3d(0,0,0); }
.check-tgl label .tx-on { display:none; padding-left:13px; font-size:11px; line-height:30px; font-weight:bold; color:#fff; }
.check-tgl label .tx-off { display:block; padding-right:13px; font-size:11px; line-height:30px; font-weight:bold; color:#848484; text-align:right; }
.check-tgl input:checked + label { background:#4fc1e9; }
.check-tgl input:checked + label .round { transform:translate3d(39px,0,0);  }
.check-tgl input:checked + label .tx-on { display:block; }
.check-tgl input:checked + label .tx-off { display:none; }
.check-tgl.disabled .round { display:none; }
.check-tgl.disabled .tx-on, .check-tgl.disabled .tx-off { padding:0; text-align:center; }

/* select 디자인 적용 - 미사용시 주석 처리 - inp-select 는 단순 dropdown / new-select 는 기존 select 요소 활용 */
.inp-select, .new-select { position:relative; height:33px; z-index:100; } /* 너비 및 높이는 직접 설정 */
.new-select > select { display:none; }
.inp-select .btn-select, .new-select .btn-select { display:block; padding:0 30px 0 9px; width:100%; height:100%; border:1px solid #b5b5b5; text-align:left; background:#fff url(/images/bg_select.png) no-repeat right 50%; border-radius:4px; }
.inp-select .btn-select[disabled], .new-select .btn-select[disabled] { opacity:.5; filter: alpha(opacity=50);}
.inp-select .btn-select > span, .new-select .btn-select > span { position:relative; font-size:13px; line-height:31px; }
.inp-select .select-list, .new-select .select-list { position:absolute; display:none; width:100%; max-height:300px; top:33px; left:0; padding:8px; border:1px solid #b5b5b5; background:#fff; box-sizing:border-box; overflow-x:hidden; overflow-y:auto; border-radius:4px; } /* 리스트 부분은 전체 너비 및 높이에서 2px 빼고 설정 */
.inp-select .select-list li button, .new-select .select-list li button { display:block; width:100%; padding:6px 10px; color:#666; text-align:left; }
.inp-select .select-list li button:hover, .new-select .select-list li button:hover { background:#fafbfc; }

.inp-select.double-line .select-list, .new-select.double-line .select-list { width:200%; }
.inp-select.double-line li, .new-select.double-line li { float:left; width:50%; }

/* 달력기능 - width 값 변경 시 별도 설정 */
#calendar table { table-layout:auto; }
.inp-cal:after { content:''; display:block; clear:both; }
.inp-cal input { max-width:100px; vertical-align:top; text-align:center; }
.inp-cal > span { display:block; float:left; width:100px; }
.inp-cal > i { display:block; float:left; width:16px; line-height:34px; text-align:center; }
.inp-cal .btn-cal { width:30px; height:34px; padding-top:5px; line-height:34px; vertical-align:-3px; }

/* 파일첨부 */
.inp-file { position:relative; display:inline-block; vertical-align:top; }
.inp-file input { position:absolute; width:1px; height:1px; padding:0; top:3px; left:3px; z-index:-1; overflow:hidden; opacity:0; }
.inp-file label { display:inline-block; padding:7px 5px 9px 30px; font-size:12px; color:#777; background:url(/images/ico_file.png) no-repeat 10px 8px; cursor:pointer; }
.inp-file label.on { color:#ea002c; font-weight:bold; background-position:10px -57px; }

/* 파일첨부 type2 */
.inp-file-type2 { position:relative; display:inline-block; width:100%; min-width:200px; height:31px; padding-left:90px; vertical-align:middle; font-size:13px; line-height:31px; box-sizing:border-box; }
.inp-file-type2 input[type=file] { position:absolute; width:1px; height:1px; padding:0; top:0; left:0; z-index:-1; opacity:0; overflow:hidden; }
